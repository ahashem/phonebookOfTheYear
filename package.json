    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.13.2",
    "whatwg-fetch": "^3.0.0"
  },
  "prettier": {
    "trailingComma": "none"
  },
  "pre-commit": []
}
